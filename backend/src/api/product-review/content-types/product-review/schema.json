{
  "kind": "collectionType",
  "collectionName": "product_reviews",
  "info": {
    "singularName": "product-review",
    "pluralName": "product-reviews",
    "displayName": "productReview",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Review": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "product_review"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_reviews"
    }
  }
}
